yo stage = "unknown";
yo age = 29;

hmm (age < 18) {
    hmm (age < 13) { 
      stage = "child"; 
    } mid (age < 16) { 
      stage = "young teenager"; 
    } nah { 
      stage = "teenager"; 
    }
} mid (age < 65) {
    hmm (age < 30) { 
      stage = "young adult"; 
    } mid (age < 50) { 
      stage = "adult"; 
    } nah { 
      stage = "middle-aged adult"; 
    }
} nah { 
  stage = "senior"; 
}
yap(stage);

yo isAdult = age >= 18;

hmm (isAdult) {
    yap("eligible for voting: true");

    hmm (age < 25) {
      yap("first-time voter: likely");
    } nah { 
      yap("first-time voter: unlikely"); 
    }
} nah { 
  yap("eligible for voting: false"); 
}

hmm (age < 16) { 
  yap("eligible for driving: false"); 
} mid (age < 18) {
    yap("eligible for driving: learner's permit");

    hmm (age < 17) { 
      yap("supervised driving required"); 
    } nah { 
      yap("unsupervised driving allowed with restrictions"); 
    }
} nah { 
  yap("eligible for driving: full license"); 
}

hmm (age < 21) { 
  yap("eligible for drinking (US): false"); 
} nah {
  yap("eligible for drinking (US): true");
  
  hmm (age < 25) { 
    yap("remember: drink responsibly!"); 
  }
}
